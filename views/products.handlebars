{{#each productlistings}}

    <div class="product-listings">
        <div class="proo">
        <img src="{{this.imgSrc}}">
        </div>
        <p>
            Title: {{this.title}}
        </p>
        <p>
            Price: {{this.price}}
        </p>
        <p>
            Description: {{this.description}}
        </p>
        
    </div>

{{/each}}